{
  "name": "use-cached-formik-field",
  "version": "0.0.0-development",
  "description": "Wrapper for Formik useField with cached value and validation result",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "commit": "git-cz",
    "build": "tsc",
    "prepare": "install-peers -f",
    "lint": "eslint -c eslint.js ./src/**/*.ts ./src/**/*.tsx",
    "lint:fix": "eslint -c eslint.js ./src/**/*.ts ./src/**/*.tsx --fix",
    "test": "jest --clearCache; NODE_ENV=test jest  --runInBand .",
    "test:coverage": "jest --clearCache; NODE_ENV=test jest --runInBand . --coverage"
  },
  "files": [
    "/lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gargoyle-unit/use-cached-formik-field"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gargoyle-unit/use-cached-formik-field/issues"
  },
  "homepage": "https://github.com/gargoyle-unit/use-cached-formik-field#readme",
  "dependencies": {
    "ts-debounce": "^2.0.1"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "formik": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@typescript-eslint/parser": "^2.25.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.35",
    "@types/react": "^16.9.34",
    "commitizen": "^4.0.4",
    "core-js": "^3.6.5",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "gts": "^1.1.2",
    "install-peers-cli": "^2.2.0",
    "jest": "25.2.1",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "^25.3.1",
    "typescript": "^3.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
